{
  "name": "ngx-cbp-theme",
  "version": "2.0.0",
  "main": "./bundles/ngx-cbp-theme.umd.js",
  "module": "./ngx-cbp-theme.es5.js",
  "es2015": "./ngx-cbp-theme.js",
  "typings": "./ngx-cbp-theme.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/US-CBP/ngx-cbp-theme.git"
  },
  "keywords": [
    "angular"
  ],
  "license": "CC0-1.0",
